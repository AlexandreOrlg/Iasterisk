{
  "name": "Iasterisk",
  "version": "1.0.0",
  "description": "Yet another new spelling corrector powered by AI",
  "main": "./out/main/index.js",
  "author": "Alexandre Orliaguet",
  "homepage": "/",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-forge start",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "@floating-ui/core": "^1.3.1",
    "@sveltejs/kit": "^1.22.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "ajv": "^8.12.0",
    "axios": "^1.4.0",
    "diff": "^5.1.0",
    "electron-store": "^8.1.0",
    "lucide-svelte": "^0.259.0",
    "openai": "^3.3.0",
    "svelte-floating-ui": "^1.5.3",
    "svelte-i18n": "^3.7.0"
  },
  "devDependencies": {
    "@danielsharkov/svelte-router": "^4.0.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "autoprefixer": "^10.4.14",
    "electron": "^24.6.2",
    "electron-builder": "^24.4.0",
    "electron-data-storage": "^1.1.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-vite": "^1.0.25",
    "env-cmd": "^10.1.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.25",
    "prettier": "^2.8.8",
    "svelte": "^3.59.2",
    "tailwindcss": "^3.3.2",
    "vite": "^4.4.2"
  }
}
